<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SquaredLayout">
        <attr name="square" format="integer">
            <enum name="vertically" value="0"/>
            <enum name="horizontally" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="ImageListView">
        <attr name="mode" format="integer">
            <enum name="vertically" value="0"/>
            <enum name="horizontally" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="NumberPicker">
        <attr name="minValue" format="integer"/>
        <attr name="maxValue" format="integer"/>
        <attr name="step" format="integer"/>
        <attr name="LtoR_order" format="integer">
            <enum name="ascending" value="0"/>
            <enum name="descending" value="1"/>
        </attr>
        <attr name="startColor" format="reference|color"/>
        <attr name="endColor" format="reference|color"/>
    </declare-styleable>
</resources>